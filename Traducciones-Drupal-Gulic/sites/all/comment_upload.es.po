#: comment_upload.module:24
msgid "Attachment"
msgstr "Adjunto"

#: comment_upload.module:26
msgid "You already have a file uploaded, if you upload another it will overwrite the current one."
msgstr "Acaba de subir un fichero, si vuelve a subir otro fichero, sobreescribirá el primero."

#: comment_upload.module:36
msgid "File attachments"
msgstr "Archivos adjuntos"

#: comment_upload.module:39
msgid "Changes made to the attachments are not permanent until you save this post."
msgstr "Los cambios realizados a los adjuntos no serán permanentes hasta que salve este envio."

#: comment_upload.module:109;117
msgid "Attachments on comments"
msgstr "Adjuntos en los comentarios"

#: comment_upload.module:111
msgid "Disabled"
msgstr "Desactivado"

#: comment_upload.module:111
msgid "Enabled"
msgstr "Activado"

#: comment_upload.module:119
msgid "Multiple"
msgstr "Múltiple"

#: comment_upload.module:119
msgid "Single"
msgstr "Simple"

#: comment_upload.module:120
msgid "Set whether to allow only one attachment per comment"
msgstr "Establecer si permite o no un solo adjunto por comentario"

#: comment_upload.module:124
msgid "Images on comments"
msgstr "Imágenes en comentarios"

#: comment_upload.module:126
msgid "Normal attachment"
msgstr "Adjunto normal"

#: comment_upload.module:126
msgid "Inline display"
msgstr "Mostrado Inline"

#: comment_upload.module:127
msgid "Show uploaded image with comment or use a normal attachment link (single attachment mode only)"
msgstr "Mostrar la imagen que ha subido con comentarios o usar un enlace normal (únicamente en modo adjunto único)"

#: comment_upload.info:0
msgid "Comment upload"
